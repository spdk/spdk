[global]
thread=1
direct=1
norandommap=1
time_based=1
do_verify=1
verify_fatal=1
verify_dump=1
verify=crc32c
verify_backlog=1024
random_distribution=normal
iodepth=128
bs=4K
runtime=999999

size=20%

[job_1]
offset=0
rw=randwrite
name=randwrite

[job_2]
offset=20%
rw=randrw
name=randrw

[job_3]
offset=40%
rw=write
name=write

[job_4]
offset=60%
rw=rw
name=rw

[job_5]
offset=80%
rw=randwrite
name=randwrite
