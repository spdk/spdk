[global]
thread=1
group_reporting=1
direct=1
norandommap=1
time_based=1
do_verify=1
verify=md5
verify_backlog=1024
iodepth=128
bs=4K
runtime=10
size=10%

[job_randwrite]
rw=randwrite
name=randwrite

[job_randrw]
offset=10%
rw=randrw
name=randrw

[job_write]
offset=20%
rw=write
name=write

[job_rw]
offset=30%
rw=rw
name=rw
