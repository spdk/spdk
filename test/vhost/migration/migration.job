[global]
blocksize_range=4k-512k
#bs=512k
iodepth=128
ioengine=libaio
filename=
group_reporting
thread
numjobs=1
direct=1
rw=randwrite
do_verify=1
verify=md5
verify_fatal=1
verify_dump=1
size=100%

[write-1]
rw=write
stonewall

[read-1]
rw=read
stonewall

[randread-1]
rw=randread
stonewall

[read-2]
rw=read
runtime=10
time_based
stonewall
