[global]
blocksize=4k
iodepth=512
iodepth_batch=128
iodepth_low=256
ioengine=libaio
size=20G
io_size=4G
group_reporting
thread
numjobs=1
direct=1
rw=randwrite
do_verify=1
verify=md5
verify_backlog=1024

[nvme-hostb]
filename=/dev/sdb

[nvme-hostc]
filename=/dev/sdc

[nvme-hostd]
filename=/dev/sdd

[nvme-hoste]
filename=/dev/sde
