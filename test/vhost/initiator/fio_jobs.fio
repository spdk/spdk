[global]
thread=1
group_reporting=1
direct=1
norandommap=1
time_based=1
do_verify=1
verify=md5
verify_backlog=1024
iodepth=128
bs=4K
size=100m
io_size=400m
filename=VirtioScsi0t0:VirtioScsi1t0:VirtioScsi2t0:

[trim_sequential]
stonewall
rw=trim
trim_verify_zero=1
name=trim_sequential

[trim_random]
stonewall
rw=randtrim
trim_verify_zero=1
name=trim_random

[write]
stonewall
rw=write
name=write

[fio_job_4G_randwrite]
size=1G
io_size=4G
offset=4G
stonewall
rw=randwrite
name=4G_randwrite

[fio_job_4G_write]
size=1G
io_size=4G
offset=4G
stonewall
rw=randwrite
name=4G_write

[fio_job_4G_rw]
size=1G
io_size=4G
offset=4G
stonewall
rw=randwrite
name=4G_rw

[fio_job_4G_randrw]
size=1G
io_size=4G
offset=4G
stonewall
rw=randwrite
name=4G_randrw

[fio_job_guest]
stonewall
rw=randwrite
filename=
name=guest_randwrite
