[global]
thread=1
group_reporting=1
direct=1
norandommap=1
time_based=1
do_verify=1
verify=md5
verify_backlog=1024
iodepth=128
bs=4K
size=100m
io_size=400m
filename

[fio_job_unmap_trim_sequential]
stonewall
rw=trim
trim_verify_zero=1
name=unmap_trim_sequential

[fio_job_unmap_trim_random]
stonewall
rw=randtrim
trim_verify_zero=1
name=unmap_trim_random

[fio_job_unmap_write]
stonewall
rw=write
name=unmap_write

[fio_job_4G_randwrite]
size=1G
io_size=4G
offset=4G
stonewall
rw=randwrite
name=4G_randwrite

[fio_job_4G_write]
size=1G
io_size=4G
offset=4G
stonewall
rw=randwrite
name=4G_write

[fio_job_4G_rw]
size=1G
io_size=4G
offset=4G
stonewall
rw=randwrite
name=4G_rw

[fio_job_4G_randrw]
size=1G
io_size=4G
offset=4G
stonewall
rw=randwrite
name=4G_randrw

[fio_job_guest_randwrite]
stonewall
rw=randwrite
name=guest_randwrite

[fio_job_guest_randrw]
stonewall
rw=randwrite
name=guest_randrw

[fio_job_guest_write]
stonewall
rw=randwrite
name=guest_write

[fio_job_guest_rw]
stonewall
rw=randwrite
name=guest_rw

[fio_job_host_randwrite]
stonewall
rw=randwrite
name=host_rardwrite

[fio_job_host_randrw]
stonewall
rw=randrw
name=host_randrw

[fio_job_host_write]
stonewall
rw=write
name=host_write

[fio_job_host_rw]
stonewall
rw=rw
name=host_rw
