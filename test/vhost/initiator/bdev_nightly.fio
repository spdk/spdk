[global]
thread=1
group_reporting=1
direct=1
norandommap=1
time_based=1
do_verify=1
verify=md5
verify_backlog=1024
iodepth=128
bs=4K

[job_randwrite]
stonewall
rw=randwrite
name=rardwrite

[job_randrw]
stonewall
rw=randrw
name=randrw

[job_write]
stonewall
rw=write
name=write

[job_rw]
stonewall
rw=rw
name=rw

[job_unmap_trim_sequential]
stonewall
rw=trim
trim_verify_zero=1
name=unmap_trim_sequential

[job_unmap_trim_random]
stonewall
rw=randtrim
trim_verify_zero=1
name=unmap_trim_random

[job_unmap_write]
stonewall
rw=write
name=unmap_write
