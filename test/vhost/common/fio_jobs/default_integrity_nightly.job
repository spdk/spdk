[global]
bsrange=4k-512k
iodepth=512
ioengine=libaio
size=10G
io_size=20G
filename=
group_reporting
thread
numjobs=1
direct=1
do_verify=1
verify=md5
verify_backlog=1024

[read]
stonewall
rw=read

[write]
stonewall
rw=write

[rw]
stonewall
rw=rw

[randread]
stonewall
rw=randread

[randwrite]
stonewall
rw=randwrite

[randrw]
stonewall
rw=randrw
