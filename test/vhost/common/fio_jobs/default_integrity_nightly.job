[global]
blocksize=256k
iodepth=64
ioengine=libaio
size=1G
io_size=4G
filename=
group_reporting
thread
numjobs=1
direct=1
do_verify=1
verify=md5
verify_backlog=1024
[nvme-host]

[read]
stonewall
rw=read

[write]
stonewall
rw=write

[rw]
stonewall
rw=rw

[randread]
stonewall
rw=randread

[randwrite]
stonewall
rw=randwrite

[randrw]
stonewall
rw=randrw

