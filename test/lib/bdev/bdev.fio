[global]
ioengine=spdk
spdk_conf=./test/lib/bdev/bdev.conf
thread=1
group_reporting=1
direct=1
norandommap=1
percentile_list=50:99:99.9:99.99:99.999
iodepth=8
bs=4k
runtime=10

[randrw_malloc0]
filename=Malloc0
rw=randrw
time_based=1
ramp_time=0
verify=sha1

[randrw_split0]
filename=Malloc1p0
rw=randrw
time_based=1
ramp_time=0
verify=sha1

[randrw_nvme0]
filename=Nvme0n1
rw=randrw
time_based=1
ramp_time=0
verify=sha1
