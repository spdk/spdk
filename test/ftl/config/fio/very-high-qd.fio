[global]
ioengine=spdk_bdev
spdk_json_conf=${FTL_JSON_CONF}
filename=${FTL_BDEV_NAME}
cpus_allowed=12
cpus_allowed_policy=split
direct=1
thread=1
buffered=0
norandommap=1
randrepeat=0
scramble_buffers=1

[WRITE]
bs=64k
numjobs=1
rw=write
iodepth=1024
runtime=3600s
time_based=1

[READ]
bs=4k
numjobs=1
rw=randread
iodepth=1024
runtime=3600s
time_based=1
