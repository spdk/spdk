[global]
ioengine=spdk_blobfs
spdk_conf=./examples/blobfs/fio_plugin/bdev.conf.in
bdev_name=Nvme0n1
thread=1
group_reporting=1
direct=1
verify=0
numjobs=1
ramp_time=0
stonewall

[randrw_4k]
runtime=5
iodepth=128
rw=randrw
bs=4k
filesize=1G

[multifile]
runtime=5
iodepth=128
rw=randrw
bs=4k
filesize=512M
filename=file1:file2
