[project]
	description = Storage Performance Development Kit - http://spdk.io
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group SPDK Maintainers
	label-Verified = -1..+1 group SPDK Bots
	editAssignee = group SPDK Leads
	editAssignee = group SPDK Maintainers
	exclusiveGroupPermissions = editAssignee
	publishDrafts = group SPDK Leads
	editHashtags = group SPDK Developers
	label-Chandler-Test-Pool = -1..+1 group SPDK Bots
[access "refs/for/refs/*"]
	read = group SPDK Maintainers
	push = group SPDK Maintainers
	label-Code-Review = -2..+2 group SPDK Maintainers
	label-Verified = -1..+1 group SPDK Maintainers
	rebase = group SPDK Maintainers
	removeReviewer = group SPDK Developers
	removeReviewer = group SPDK Maintainers
	submit = group SPDK Maintainers
	editAssignee = group SPDK Leads
	editAssignee = group SPDK Maintainers
	exclusiveGroupPermissions = editAssignee
[access "refs/heads/*"]
	read = group SPDK Maintainers
	create = group SPDK Maintainers
	push = group SPDK Maintainers
[access "refs/tags/*"]
	push = +force group SPDK Maintainers
	create = group SPDK Maintainers
	createTag = group SPDK Maintainers
	createSignedTag = group SPDK Maintainers
[receive]
	requireSignedOffBy = true
	requireChangeId = true
	rejectImplicitMerges = true
[submit]
	action = rebase always
[access "refs/for/*"]
	exclusiveGroupPermissions = addPatchSet
	addPatchSet = group SPDK Developers
	addPatchSet = group SPDK Maintainers
[label "Code-Review"]
	copyMinScore = true
	copyAllScoresOnTrivialRebase = true
	function = MaxWithBlock
	value = -2 This shall not be merged
	value = -1 I would prefer this is not merged as is
	value =  0 No score
	value = +1 Looks good to me, but someone else must approve
	value = +2 Looks good to me, approved
	defaultValue = 0
[access "refs/meta/config"]
	read = group SPDK Maintainers
	push = group SPDK Maintainers
	create = group SPDK Maintainers
[label "Chandler-Test-Pool"]
	function = NoBlock
	value = -2 Do not use
	value = -1 Failed
	value =  0 No score
	value = +1 Passed
	value = +2 Do not use
	defaultValue = 0
