[project]
	description = Storage Performance Development Kit - http://spdk.io
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group SPDK Maintainers
[access "refs/for/refs/*"]
	read = group user/Daniel Verkamp (dverkamp-intel)
	push = group user/Daniel Verkamp (dverkamp-intel)
	label-Code-Review = -2..+2 group user/Daniel Verkamp (dverkamp-intel)
	label-Verified = -1..+1 group SPDK Maintainers
	rebase = group user/Daniel Verkamp (dverkamp-intel)
	removeReviewer = group user/Daniel Verkamp (dverkamp-intel)
	submit = group user/Daniel Verkamp (dverkamp-intel)
[access "refs/heads/*"]
	read = group user/Daniel Verkamp (dverkamp-intel)
	create = group user/Daniel Verkamp (dverkamp-intel)
	push = +force group user/Daniel Verkamp (dverkamp-intel)
	pushMerge = group user/Daniel Verkamp (dverkamp-intel)
[access "refs/tags/*"]
	push = group user/Daniel Verkamp (dverkamp-intel)
	push = +force group user/Daniel Verkamp (dverkamp-intel)
[receive]
	requireSignedOffBy = true
[submit]
	action = rebase if necessary
